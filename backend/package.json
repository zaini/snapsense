{
  "name": "snapsense-backend",
  "version": "1.0.0",
  "description": "The backend for SnapSense",
  "main": "index.js",
  "scripts": {
    "serve": "node server.js",
    "start": "nodemon server.js",
    "test": "jest --config ./tests/jest.config.js --forceExit --detectOpenHandles",
    "db:drop:test": "cross-env NODE_ENV=test npx sequelize-cli db:drop",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "db:migrate:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate",
    "db:seed:test": "cross-env NODE_ENV=test npx sequelize-cli db:seed:all",
    "db:test": "npm run db:drop:test && npm run db:create:test && npm run db:migrate:test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.5.0",
    "@ladjs/graceful": "^1.0.5",
    "apollo-server": "^2.21.0",
    "apollo-server-errors": "^2.4.2",
    "apollo-server-express": "^2.21.0",
    "argon2": "^0.27.1",
    "aws-sdk": "^2.850.0",
    "bcrypt": "^5.0.0",
    "bree": "^6.2.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "email-templates": "^8.0.3",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.0.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.4.18",
    "nodemailer-smtp-transport": "^2.7.4",
    "p-map": "^4.0.0",
    "pug": "^3.0.2",
    "sequelize": "^6.5.0",
    "sequelize-auto-migrations": "^1.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "proxyquire": "^2.1.3",
    "sequelize-cli": "^6.2.0",
    "sequelize-mock": "^0.10.2",
    "supertest": "^6.1.3"
  }
